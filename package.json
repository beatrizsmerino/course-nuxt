{
	"version": "1.0.0",
	"private": true,
	"name": "nuxt-course",
	"description": "Udemy course on Nuxt Framework.",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"dependencies:fix": "npm install --ignore-scripts && npx npm-force-resolutions",
		"preinstall": "npm run install:clean && npm run dependencies:fix",
		"postinstall": "npm run lint",
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\" ./.prettierrc",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path ./.eslintignore",
		"stylelint:fix": "stylelint \"./**/*.{css,scss,sass,vue}\" --fix --ignore-path ./.stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"test": "jest"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/svg-sprite": "^0.5.2",
		"core-js": "^3.35.0",
		"nuxt": "^2.16.0",
		"vue": "^2.7.14",
		"vue-server-renderer": "^2.7.16",
		"vue-template-compiler": "^2.7.16",
		"vuex": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.23.7",
		"@babel/eslint-parser": "^7.23.3",
		"@nuxt/babel-preset-app": "^2.17.3",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/style-resources": "^1.2.2",
		"@nuxtjs/stylelint-module": "^4.2.1",
		"@vue/test-utils": "^1.3.6",
		"autoprefixer": "^10.4.17",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^29.7.0",
		"browserslist": "^4.22.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^27.6.3",
		"eslint-plugin-jsonc": "^2.12.2",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vue": "^9.20.1",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8.4.33",
		"postcss-html": "^1.6.0",
		"prettier": "^3.2.4",
		"sass": "^1.70.0",
		"sass-loader": "^10.4.1",
		"stylelint": "^15.10.3",
		"stylelint-config-property-sort-order-smacss": "^10.0.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-sass-guidelines": "^10.0.0",
		"stylelint-config-standard": "^34.0.0",
		"vue-jest": "^3.0.7"
	},
	"resolutions": {
		"glob-parent": "6.0.2",
		"json5": "2.2.3",
		"nth-check": "2.1.1"
	},
	"volta": {
		"node": "18.19.0",
		"npm": "10.2.3"
	}
}
