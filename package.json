{
	"name": "nuxt-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path .gitignore .",
