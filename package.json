{
	"version": "1.0.0",
	"private": true,
	"name": "nuxt-course",
	"description": "Udemy course on Nuxt Framework.",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"dependencies:fix": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"preinstall": "npm run install:clean && npm run dependencies:fix",
		"postinstall": "npm run lint",
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\" ./.prettierrc",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path ./.eslintignore",
		"stylelint:fix": "stylelint \"./**/*.{css,scss,sass,vue}\" --fix --ignore-path ./.stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"test": "jest"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/svg-sprite": "^0.5.2",
		"core-js": "^3.32.1",
		"nuxt": "^2.16.0",
		"vue": "^2.7.14",
		"vue-server-renderer": "^2.7.14",
		"vue-template-compiler": "^2.7.14",
		"vuex": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/eslint-parser": "^7.22.9",
		"@nuxt/babel-preset-app": "^2.17.1",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/style-resources": "^1.2.1",
		"@nuxtjs/stylelint-module": "^4.2.1",
		"@vue/test-utils": "^1.3.6",
		"autoprefixer": "^10.4.14",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^29.7.0",
		"browserslist": "^4.21.10",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-jest": "^27.4.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.16.1",
		"jest": "^29.6.2",
		"jest-environment-jsdom": "^29.6.2",
		"postcss": "^8.4.29",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.8",
		"sass": "^1.67.0",
		"sass-loader": "^10.4.1",
		"stylelint": "^15.10.2",
		"stylelint-config-property-sort-order-smacss": "^9.1.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-sass-guidelines": "^10.0.0",
		"stylelint-config-standard": "^34.0.0",
		"vue-jest": "^3.0.7"
	},
	"resolutions": {
		"glob-parent": ">=5.1.2",
		"json5": ">=2.2.2",
		"nth-check": ">=2.0.1"
	},
	"volta": {
		"node": "14.21.3",
		"npm": "6.14.18"
	}
}
