{
	"name": "nuxt-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"preinstall": "npm run install:clean && npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path .gitignore .",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\"",
		"lint": "npm run prettier:fix && npm run eslint:fix"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/svg-sprite": "^0.5.2",
		"core-js": "^3.26.1",
		"nuxt": "^2.15.8"
	},
	"devDependencies": {
		"@nuxtjs/eslint-config": "^6.0.1",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/style-resources": "^1.2.1",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-nuxt": "^3.2.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.8.0",
		"prettier": "^2.8.0",
		"sass": "^1.56.1",
		"sass-loader": "^10.1.1"
	},
	"resolutions": {
		"glob-parent": ">=5.1.2",
		"nth-check": ">=2.0.1",
		"parse-path": ">=5.0.0"
	},
	"volta": {
		"node": "14.19.3",
		"npm": "6.14.17"
	}
}
