{
	"name": "nuxt-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"preinstall": "npm run install:clean && npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path .eslintignore",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\" .prettierrc",
		"lint": "npm run prettier:fix && npm run eslint:fix"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/svg-sprite": "^0.5.2",
		"core-js": "^3.27.2",
		"nuxt": "^2.15.8"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.19.1",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/style-resources": "^1.2.1",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"prettier": "^2.8.3",
		"sass": "^1.57.1",
		"sass-loader": "^10.4.1"
	},
	"resolutions": {
		"glob-parent": ">=5.1.2",
		"json5": ">=2.2.2",
		"nth-check": ">=2.0.1",
		"parse-path": ">=5.0.0"
	},
	"volta": {
		"node": "14.19.3",
		"npm": "6.14.17"
	}
}
