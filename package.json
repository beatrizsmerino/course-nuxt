{
  "name": "nuxt-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:clean": "rm -rf node_modules package-lock.json",
    "preinstall": "npm run install:clean && npm install --ignore-scripts && npx npm-force-resolutions",
    "postinstall": "npm run lintfix && npm run lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "core-js": "^3.27.2",
    "nuxt": "^2.16.0",
    "vue": "^2.7.14",
    "vue-server-renderer": "^2.7.14",
    "vue-template-compiler": "^2.7.14",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@vue/test-utils": "^1.3.4",
    "babel-core": "7.0.0-bridge.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.4.2",
    "browserslist": "^4.21.5",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "sass-loader": "^10.4.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "vue-jest": "^3.0.7"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "json5": ">=2.2.2",
    "nth-check": ">=2.0.1"
  },
  "volta": {
    "node": "14.21.2",
    "npm": "6.14.18"
  }
}
